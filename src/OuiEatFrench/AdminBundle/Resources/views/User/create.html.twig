{% extends 'OuiEatFrenchAdminBundle::layout.html.twig' %}

{% block title %}
<h3>Creation d'un Administrateur</h3>
{% endblock %}
{% block content %}
<form action="{{ path(route) }}" method="post" {{ form_enctype(form) }} novalidate="novalidate">
    {% for child in form.children %}
        {{ form_row(child) }}
    {% endfor %}
    <br>
    <input class="btn btn-success" type="submit" value="submit"/>
</form>
{% endblock %}
