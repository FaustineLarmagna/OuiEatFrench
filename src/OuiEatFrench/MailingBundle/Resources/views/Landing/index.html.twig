<script type="text/javascript">
    $(function ()
    {
        var div = $('#headerwrap #slider');
        var backgrounds = [
            "url({{ asset('ouieatfrench/theme/assets2/img/murephoto2.jpg') }})",
            "url({{ asset('ouieatfrench/theme/assets2/img/coccinelle.jpg') }})",
            "url({{ asset('ouieatfrench/theme/assets2/img/abricot.jpg') }})",
            "url({{ asset('ouieatfrench/theme/assets2/img/fleur.jpg') }})"];
        var current = 0;

        function nextBackground() {
            div.fadeOut(100, function(){
                div.css(
                        'background-image',
                        backgrounds[current = ++current % backgrounds.length]);
                div.css('background-size','cover');
                div.css('background-position', 'center center');
                div.fadeIn(1000);
            });


            setTimeout(nextBackground, 7000);
        }
        setTimeout(nextBackground, 7000);
        div.css('background-image', backgrounds[0]);
        div.css('background-size','cover');
        div.css('background-position', 'center center');
    });

    $(document).ready(function() {
        $('.fancybox-media')
                .attr('rel', 'media-gallery')
